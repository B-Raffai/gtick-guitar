#: src/gtick.c:81
#, c-format
msgid ""
"Usage: %s [OPTION...]\n"
"\n"
"Options:\n"
"  -h, --help              Show this help message\n"
"  -v, --version           Print version information\n"
"  -d, --debug[=level]     Print additional runtime debugging data to stdout\n"
"\n"
msgstr ""
"Використання: %s [ПАРАМЕТР...]\n"
"\n"
"Параметри:\n"
"  -h, --help\tПоказати це повідомлення\n"
"  -v, --version\tПоказати інформацію про версію\n"
"  -d, --debug[=level]\tДрукувати додаткову інформацію, корисну для "
"ліквідації помилок\n"
"\n"

